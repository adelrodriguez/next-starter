    "@types/bun": "1.1.6",
    "@types/node": "20.14.11",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "drizzle-kit": "0.23.0",
    "jiti": "1.21.6",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.39",
    "react-email": "2.1.5",
