{
  "name": "startline",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "check": "biome check --write src",
    "clean:build": "rm -rf .next",
    "clean:data": "rm -rf .data/*",
    "clean:migrations": "rm -rf migrations/*",
    "clean": "run-p clean:*",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "deps:check": "taze",
    "deps:check:major": "taze major",
    "deps:update": "taze -w",
    "dev:email": "cp .env ./node_modules/react-email/ && email dev --dir src/components/emails --port 3001",
    "dev": "next dev",
    "docker:down": "docker-compose down",
    "docker:up": "docker-compose up -d",
    "format": "biome format --write src",
    "lint": "biome lint --fix src",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@conform-to/react": "1.1.5",
    "@conform-to/zod": "1.1.5",
    "@epic-web/remember": "1.1.0",
    "@libsql/client": "0.7.0",
    "@lucia-auth/adapter-drizzle": "1.0.7",
    "@node-rs/argon2": "1.8.3",
    "@react-email/components": "0.0.21",
    "@react-email/render": "0.0.17",
    "@sentry/nextjs": "8.19.0",
    "@t3-oss/env-core": "0.10.1",
    "@t3-oss/env-nextjs": "0.10.1",
    "@upstash/ratelimit": "2.0.1",
    "@upstash/redis": "1.33.0",
    "@vercel/analytics": "1.3.1",
    "arctic": "1.9.2",
    "clsx": "2.1.1",
    "date-fns": "3.6.0",
    "drizzle-orm": "0.32.0",
    "http-status-codes": "2.3.0",
    "inngest": "3.22.0",
    "lucia": "3.2.0",
    "next": "14.2.5",
    "oslo": "1.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "resend": "3.5.0",
    "server-only": "0.0.1",
    "stripe": "16.2.0",
    "tailwind-merge": "2.4.0",
    "zod": "3.23.8"
