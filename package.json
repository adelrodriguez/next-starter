{
  "name": "saas-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "check": "biome check --apply src",
    "clean:db": "rm -rf local.db*",
    "clean:migrations": "rm -rf migrations/*",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:run": "turso dev --db-file=local.db",
    "db:studio": "drizzle-kit studio",
    "dev:db": "turso dev --db-file=local.db",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "format": "biome format --write src",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@epic-web/remember": "1.1.0",
    "@libsql/client": "^0.7.0",
    "@next/bundle-analyzer": "^14.2.5",
    "@sentry/nextjs": "^8.19.0",
    "@t3-oss/env-core": "^0.10.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tailwindcss/typography": "^0.5.13",
    "@vercel/analytics": "^1.3.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.32.0",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/node": "20.14.11",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "drizzle-kit": "^0.23.0",
    "jiti": "^1.21.6",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.39",
    "tailwindcss": "3.4.6",
    "typescript": "5.5.3"
  }
}
